{
  "name": "playwright-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:aws": "rimraf allure-results && npx playwright test tests/aws --reporter=line,allure-playwright",
    "test:s4": "rimraf allure-results && npx playwright test tests/s4 --reporter=line,allure-playwright",
    "test:allure": "rimraf allure-results && npx playwright test --reporter=line,allure-playwright",
    "allure:generate": "npx allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "npx allure open ./allure-report",
    "allure:serve": "npx allure serve ./allure-results"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prasanthvb/playwright-api-test.git"
  },
